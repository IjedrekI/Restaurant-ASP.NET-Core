@page
@model Restaurant.Pages.Restaurants.ListModel
@{
    ViewData["Title"] = "List";
}

<h1>Restaurants</h1>

<table class="table">
    @foreach(var r in Model.Restaurants)
    {
        <tr>
            <td>@r.Name</td>
            <td>@r.Location</td>
            <td>@r.Cusine</td>
        </tr>
    }
</table>


@Model.Message

